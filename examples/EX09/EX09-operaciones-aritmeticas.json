                },
                {
                    "@type" : "div",
                    "@content" : "12 / 3 = ${12 / 3}"
                },
                {
                    "@type" : "all",
                    "@content" : "5 * 2 + 6 / 3 - 1"
                }
            ]
        }
    ]
}



