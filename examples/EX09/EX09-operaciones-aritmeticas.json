{
    "@type":"html",
    "@content": [
        {
            "@type": "head",
            "@content": {
                "@type": "title",
                "@content": "Arithmetic Operations"     
            }
        },
        {
            "@type": "body",
            "@content": [
                {
                    "@type" : "sum",
                    "@content" : "2 + 3 = ${2 + 3}"
                },
                {
