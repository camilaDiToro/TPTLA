{
    "@type": "body",
    "@content": {
        "@type": "@for",
        "@var": "align",
        "@in": ["right","center", {"@type": "asfa",
            "@content":"asfa"},"justify", "nnnnnnnnn"],
        "@content": {
            "@type":"div",
            "align": "${align}",
            "@content":"I am a div and my alignment is ${align}."
        }
    }
}