{
    "@type": "@for", 
    "@var": "i", 
    "@inrange": ["${1}", "${10}"],
    "@content": ["${i}", {
        "@type": "@if", 
        "@condition": "(2 > 7)*7 - 2", 
        "@then": {
            "@type": "@for", 
            "@var": "name", 
            "@in": ["gonzalo", "camila"],
            "@content": "${name}"
        }
    } 
]
}